{
  "name": "husn-muslim-scrapping",
  "version": "1.0.3",
  "description": "A sample JavaScript/NodeJs scrapper project based on husnmuslim API to use all off the texts & audios offline without the connecting with the original api",
  "main": "index.js",
  "scripts": {
    "step:1": "node index.js",
    "step:2": "node fetch.js",
    "step:3": "node merge.js",
    "step:4": "node fetch-sub.js",
    "step:1:prod": "node index.js mode=prod",
    "step:2:prod": "node fetch.js mode=prod",
    "step:3:prod": "node merge.js mode=prod",
    "step:4:prod": "node fetch-sub.js mode=prod",
    "step:1:en": "node index.js lang=en",
    "step:2:en": "node fetch.js lang=en",
    "step:3:en": "node merge.js lang=en",
    "step:4:en": "node fetch-sub.js lang=en",
    "step:1:en:prod": "node index.js mode=prod lang=en",
    "step:2:en:prod": "node fetch.js mode=prod lang=en",
    "step:3:en:prod": "node merge.js mode=prod lang=en",
    "step:4:en:prod": "node fetch-sub.js mode=prod lang=en",
    "start": "node index.js && node fetch.js mode=prod && node merge.js mode=prod && node fetch-sub.js mode=prod",
    "dev": "node index.js && node fetch.js && node merge.js && node fetch-sub.js",
    "start:en": "node index.js && node fetch.js mode=prod lang=en && node merge.js mode=prod lang=en && node fetch-sub.js mode=prod lang=en",
    "dev:en": "node index.js && node fetch.js lang=en && node merge.js lang=en && node fetch-sub.js lang=en",
    "format": "prettier . --write"
  },
  "keywords": [
    "hisn muslim",
    "hisn muslim data",
    "hisn muslim scraping",
    "scraping",
    "adhkar",
    "حصن مسلم",
    "ادكار النوم",
    "ادكار المساء",
    "ادكار الصباح"
  ],
  "author": "Mohssin Aboutaj",
  "license": "MIT",
  "dependencies": {
    "argv": "^0.0.2",
    "axios": "^0.21.1",
    "node-wget": "^0.4.3",
    "uniq-with": "^0.1.2",
    "user-agents": "^1.0.581",
    "write-json": "^3.0.1"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohssineAboutaj/hisn-muslim-scrapping.git"
  },
  "bugs": {
    "url": "https://github.com/mohssineAboutaj/hisn-muslim-scrapping/issues"
  },
  "homepage": "https://github.com/mohssineAboutaj/hisn-muslim-scrapping#readme"
}
